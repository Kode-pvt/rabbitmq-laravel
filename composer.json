{
  "name": "kode-pvt/rabbitmq-laravel",
  "version": "1.1.2",
  "type": "library",
  "license": "MIT",
  "description": "A lightweight RabbitMQ integration package for Laravel 12.",
  "keywords": [
    "laravel",
    "rabbitmq",
    "queue",
    "amqp",
    "messaging"
  ],
  "autoload": {
    "psr-4": {
      "KodePvt\\RabbitmqLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KodePvt\\RabbitmqLaravel\\RabbitmqLaravelServiceProvider"
      ],
      "aliases": {
        "RabbitMQ": "KodePvt\\RabbitmqLaravel\\Facades\\RabbitMQ"
      }
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KodePvt\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Nehal Ahmed Khan",
      "email": "nak.work.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "php-amqplib/php-amqplib": "^3.7",
    "illuminate/contracts": "^12.0",
    "illuminate/support": "^12.0",
    "illuminate/console": "^12.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.1",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^10.6",
    "pestphp/pest-plugin-laravel": "^4.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
